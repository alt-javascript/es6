  },
  "homepage": "https://github.com/alt-javascript/cookies#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/alt-javascript/cookies"
  },
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "lint": "npx eslint \"./*.js\" --fix",
