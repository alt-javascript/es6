{
  "name": "@alt-javascript/lang",
  "version": "1.0.1",
  "description": "Language feature detection utilities for ECMAScript 6 and beyond.",
  "author": "",
  "keywords": [
    "es6",
    "es7",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
